package robot_return;

public class Robot_Return_Origin_b {
	    public static boolean judgeCircle(String moves) {
	       int x = 0;
	       int y = 0;
	    for(char move:moves.toCharArray()){
	        if(move == 'U') y++;
	        else if(move == 'D') y--;
	        else if(move == 'R') x++;
	        else if(move == 'L') x--;
	        }if(x==0 && y==0){
	        return true;
	    }else{
	        return false;
	    }}
	public static void main(String[] args){
	    System.out.println(judgeCircle("UDUDR"));
	}}

	
	        
	        
	        
	        
	 
