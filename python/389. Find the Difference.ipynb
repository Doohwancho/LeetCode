    "s = \"abcd\"\n",
    "t = \"abcde\"\n",
    "\n",
    "def findTheDifference(s, t):\n",
    "    s = sorted(s)\n",
    "    t = sorted(t)\n",
    "\n",
    "    for i in range(len(s)):\n",
    "        if s[i] != t[i]:\n",
    "            return t[i]\n",
    "\n",
    "    return t[-1]\n",
    "\n",
    "findTheDifference(s,t)"
   ]
